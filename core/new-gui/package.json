  "scripts": {
    "ng": "nx",
    "start": "concurrently --kill-others \"npx y-websocket\" \"ng serve\"",
    "build": "ng build --configuration production --progress=false --source-map=false",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/stats.json",
    "test": "ng test",
    "test:ci": "ng test --watch=false --progress=false --browsers=ChromeHeadless",
    "prettier:fix": "prettier --write ./src",
    "lint": "ng lint",
    "eslint:fix": "yarn eslint --fix ./src",
    "format:fix": "yarn prettier-eslint --write \"src/**/*.{ts,js,html,scss,less,json}\"",
    "format:ci": "yarn prettier-eslint --list-different \"src/**/*.{ts,js,html,scss,less,json}\" && yarn eslint ./src",
    "e2e": "ng e2e",
    "preinstall": "cd multi-target && yarn install && yarn run build",
    "multi-target-updater": "cd multi-target && yarn run build && cd .. && yarn install ./multi-target -D",
    "postinstall": "node git-version.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points && yarn --cwd node_modules/@danmarshall/deckgl-typings run postinstall && node ./decorate-angular-cli.js"
  },
